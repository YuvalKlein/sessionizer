    "version": "0.2.0",
    "configurations": [
        {
      "name": "Flutter Web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "web-server",
        "--web-hostname",
        "0.0.0.0",
        "--web-port",
        "8080"
      ]
    },
        {
            "name": "sessionizer",
            "request": "launch",
            "type": "dart"
        },
        
        {
            "name": "sessionizer (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "sessionizer (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}